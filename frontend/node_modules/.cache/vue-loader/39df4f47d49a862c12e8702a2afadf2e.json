nted() {\n    this.$store.dispatch('todo/getTasks')\n    this.$store.dispatch('todo/getBoards')\n  }\n};\n</script>\n<style lang=\"scss\">\n.header-container {\n  max-width: none !important;\n}\n\n.container {\n  .federica-font {\n    font-family: \"Fredericka the Great\", cursive !important;\n  }\n\n  .federica-font__red {\n    font-family: \"Fredericka the Great\", cursive !important;\n    color: red;\n  }\n}\n\n.v-responsive__content {\n  display: flex;\n  justify-content: center;\n\n  .img-justify {\n    display: flex;\n    align-items: center;\n    flex-direction: column;\n  }\n}\n\n.avatar-text {\n  color: #f3f3f3;\n  z-index: 150;\n\n  &__frederica {\n    font-family: \"Fredericka the Great\", cursive;\n  }\n}\n\n.avatar-back {\n  position: relative;\n\n  &:before {\n    display: block;\n    content: \"\";\n    position: absolute;\n    left: 0;\n    right: 0;\n    top: 0;\n    bottom: 0;\n    background: rgba(0, 0, 0, 0.5);\n  }\n}\n</style>\n"]}]}