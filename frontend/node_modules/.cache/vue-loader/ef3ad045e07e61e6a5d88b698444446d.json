 <dialog-delete\n      v-if=\"dialogs.delete\"\n      @close=\"dialogs.delete = false\"\n      :task=\"task\"\n    />\n    <dialog-due-date\n      v-if=\"dialogs.dueDate\"\n      @close=\"dialogs.dueDate = false\"\n      :task=\"task\"\n    />\n     <dialog-edit\n      v-if=\"dialogs.edit\"\n      @close=\"dialogs.edit = false\"\n      :task=\"task\"\n    /> \n    \n  </div>\n</template>\n\n<script>\nexport default {\n  props: [\"task\"],\n  data: () => ({\n    dialogs: {\n      delete: false,\n      edit: false,\n      dueDate: false,\n     \n    },\n    items: [\n      {\n        title: \"Edit\",\n        icon: \"mdi-pencil-outline\",\n        click() {\n         this.dialogs.edit = true;\n        },\n      },\n      {\n        title: \"Due date\",\n        icon: \"mdi-calendar-month-outline\",\n        click() {\n         this.dialogs.dueDate = true;\n        },\n      },\n      {\n        title: \"Delete\",\n        icon: \"mdi-trash-can-outline\",\n        click() {\n          this.dialogs.delete = true;\n        },\n      },\n      {\n        title: \"Sort\",\n        icon: \"mdi-drag-horizontal-variant\",\n        click() {\n          this.$store.commit('todo/toggleSorting')\n        },\n      },     \n    ],\n  }),\n  methods: {\n    handleClick(index) {\n      this.items[index].click.call(this);\n    },\n  },\n  components: {\n    \"dialog-delete\": require(\"@/components/Dialogs/DialogDelete.vue\").default,\n    \"dialog-edit\": require(\"@/components/Dialogs/DialogEdit.vue\").default,\n    'dialog-due-date': require(\"@/components/Dialogs/DialogDueDate.vue\").default,    \n  },\n};\n</script>\n\n<style></style>\n"]}]}