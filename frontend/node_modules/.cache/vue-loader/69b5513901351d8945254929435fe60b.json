ass=\"ml-auto\" v-model=\"board.color\" type=\"color\" />\n          </v-row>\n        </div>\n      </v-card-text>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn text @click=\"$emit('close')\"> close </v-btn>\n        <v-btn @click=\"addBoard\" color=\"primary\"> Add Board </v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nexport default {\n  components: {},\n  data() {\n    return {\n      list: this.$store.state.todo.boards.length,\n      board: {\n        list: \"\",\n        title: \"\",\n        backgroundColor: \"\",\n        color: \"\",\n      },\n    };\n  },\n  methods: {\n    addBoard() {\n      this.list++;\n      this.$store.dispatch('todo/addBoard', {\n        list: this.list,\n        title: this.board.title,\n        backgroundColor: this.board.backgroundColor,\n        color: this.board.color,\n      });\n      this.title = \"\";\n      this.backgroundColor = \"\";\n      this.color = \"\";\n    },\n  },\n};\n</script>\n\n<style></style>\n"]}]}